{
  "name": "obelisk-backend",
  "version": "2.0.0",
  "description": "OBELISK DEX Backend - Ultra-Fast Trading API with Institutional Support",
  "main": "server-ultra.js",
  "scripts": {
    "start": "node server-ultra.js",
    "start:legacy": "node server.js",
    "dev": "node server-ultra.js",
    "ultra": "node server-ultra.js",
    "test": "node tests/master-test-runner.js standard",
    "test:quick": "node tests/master-test-runner.js quick",
    "test:full": "node tests/master-test-runner.js full",
    "test:auth": "node tests/auth-tests.js",
    "test:e2e": "node tests/e2e-trading-tests.js",
    "test:lending": "node tests/lending-tests.js",
    "test:performance": "node tests/performance-benchmarks.js",
    "simulate": "node user-simulator.js",
    "simulate:retail": "node tests/retail-simulation.js",
    "simulate:institutional": "node tests/institutional-simulation.js",
    "simulate:all": "node tests/master-test-runner.js simulation",
    "simulate:lending": "node lending-simulator.js",
    "simulate:full": "node full-simulation.js",
    "bench": "node tests/performance-benchmarks.js",
    "bench:stress": "node tests/master-test-runner.js performance",
    "report": "node tests/master-test-runner.js full --report",
    "backup": "node backup-db.js",
    "backup:list": "node backup-db.js --list",
    "backup:restore": "node backup-db.js --restore",
    "backup:cleanup": "node backup-db.js --cleanup",
    "validate": "node -e \"require(\"./config-validator\").validateConfig()\"",
    "start:https": "HTTPS=true node server-ultra.js"
  },
  "dependencies": {
    "@dydxprotocol/v4-client-js": "^3.4.0",
    "@gainsnetwork/sdk": "^1.7.4",
    "@sentry/node": "^10.32.1",
    "axios": "^1.6.0",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.5.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.16.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "hyperliquid": "^1.7.7",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ws": "^8.16.0",
    "yaml": "^2.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
